{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "enhancers-design-system": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/enhancers-design-system",
      "sourceRoot": "projects/enhancers-design-system/src",
      "prefix": "eds",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/enhancers-design-system/tsconfig.lib.json",
            "project": "projects/enhancers-design-system/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/enhancers-design-system/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/enhancers-design-system/src/test.ts",
            "tsConfig": "projects/enhancers-design-system/tsconfig.spec.json",
            "karmaConfig": "projects/enhancers-design-system/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/enhancers-design-system/tsconfig.lib.json",
              "projects/enhancers-design-system/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ds-storybook": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ds-storybook",
      "sourceRoot": "projects/ds-storybook/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ds-storybook",
            "index": "projects/ds-storybook/src/index.html",
            "main": "projects/ds-storybook/src/main.ts",
            "polyfills": "projects/ds-storybook/src/polyfills.ts",
            "tsConfig": "projects/ds-storybook/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/ds-storybook/src/favicon.ico",
              "projects/ds-storybook/src/assets"
            ],
            "styles": [
              "dist/enhancers-design-system/_eds.scss",
              "projects/ds-storybook/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ds-storybook/src/environments/environment.ts",
                  "with": "projects/ds-storybook/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ds-storybook:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ds-storybook:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ds-storybook:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ds-storybook/src/test.ts",
            "polyfills": "projects/ds-storybook/src/polyfills.ts",
            "tsConfig": "projects/ds-storybook/tsconfig.spec.json",
            "karmaConfig": "projects/ds-storybook/karma.conf.js",
            "assets": [
              "projects/ds-storybook/src/favicon.ico",
              "projects/ds-storybook/src/assets"
            ],
            "styles": [
              "projects/ds-storybook/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ds-storybook/tsconfig.app.json",
              "projects/ds-storybook/tsconfig.spec.json",
              "projects/ds-storybook/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ds-storybook/e2e/protractor.conf.js",
            "devServerTarget": "ds-storybook:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ds-storybook:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ds-storybook",
  "cli": {
    "analytics": "5fffabee-0b47-485c-8628-10f050c381f7"
  }
}
